        <activity android:name=".Login_front.forgetpassword" />
        <activity android:name=".Images.ImagesActivity" />
        <activity android:name=".Holder.New_Image_Activity" />
        <activity android:name=".Admin.ui.EditItems" />
        <activity android:name=".notification.SendNotification"/>
        <activity android:name=".Home.MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
        <service
            android:name=".notification.MyFirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>
            </intent-filter>
        </service>
        <service
            android:name=".notification.MyFireBaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>

    </application>

</manifest>