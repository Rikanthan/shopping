    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_shoppingcart"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity android:name=".customer.customer_view_booking"></activity>
        <activity android:name=".Views.ShowBookings" />
        <activity android:name=".customer.get_booking" />
        <activity android:name=".Views.ShowOrders" />
        <activity android:name=".Views.Individual_items" />
        <activity android:name=".customer.customer" />
        <activity android:name=".Product.Show_items_Activity" />
        <activity android:name=".category.veg_activity_new" />
        <activity android:name=".category.cosmetics_activity" />
        <activity android:name=".category.snacks_activity" />
        <activity android:name=".category.hygiene_activity" />
        <activity android:name=".category.bakery" />
        <activity android:name=".category.paper" />
        <activity android:name=".category.medical_activity" />
        <activity android:name=".category.softdrinks_activity" />
        <activity android:name=".Admin.admin_catergory" />
        <activity android:name=".Images.imageupload" />
