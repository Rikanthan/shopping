<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.login_page">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_shoppingcart"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity android:name=".Views.Timer"></activity>
        <activity android:name=".Admin.SetDateActivity" />
        <activity android:name=".customer.CustomerViewBookings" />
        <activity android:name=".Views.ShowBookings" />
        <activity android:name=".customer.GetBookings" />
        <activity android:name=".Views.ShowOrders" />
        <activity android:name=".Views.IndividualItems" />
        <activity android:name=".customer.customer" />
        <activity android:name=".Product.ShowItemsActivity" />
        <activity android:name=".category.Vegetables" />
        <activity android:name=".category.Cosmetrics" />
        <activity android:name=".category.Snacks" />
        <activity android:name=".category.Hygiene" />
        <activity android:name=".category.Bakery" />
        <activity android:name=".category.Paper" />
        <activity android:name=".category.Medical" />
        <activity android:name=".category.Softdrinks" />
        <activity android:name=".Admin.AdminCatorgory" />
        <activity android:name=".Images.imageupload" />
        <activity android:name=".Views.Softdrinks" />
        <activity android:name=".Quantitysample.mango" />
        <activity android:name=".Admin.AdminProducts" />
        <activity android:name=".Admin.Admin" />
        <activity android:name=".Views.Hygiene" />
        <activity android:name=".Views.Fruit" />
        <activity android:name=".Home.Home" />
        <activity android:name=".Login_front.SignIn" />
        <activity android:name=".Login_front.ForgetPassword" />
        <activity android:name=".Images.ImagesActivity" />
        <activity android:name=".Admin.AdminViewProducts" />
        <activity android:name=".Admin.ui.EditItems" />
        <activity android:name=".notification.SendNotification" />
        <activity android:name=".Admin.AdminViewConfirmBookings" />
        <activity android:name=".Views.ShowConfirmOrders"/>
        <activity android:name=".Home.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <service android:name=".notification.MyFirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>
        <service android:name=".notification.MyFireBaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>